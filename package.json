{
  "name": "ember-google-maps",
  "version": "4.0.0-beta.5",
  "description": "A friendly Ember addon for working with Google Maps.",
  "keywords": [
    "ember-addon",
    "emberjs",
    "google maps",
    "g-map",
    "google",
    "map",
    "maps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sandydoo/ember-google-maps.git"
  },
  "license": "MIT",
  "author": "Sander Melnikov <hey@sandydoo.me>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "homepage": "https://ember-google-maps.sandydoo.me",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:build": "node build-tests/build-test"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "broccoli-debug": "^0.6.5",
    "broccoli-funnel": ">=2.0.0",
    "camelcase": "^6.0.0",
    "chalk": "^4.1.0",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-concurrency": "^2.0.0",
    "ember-in-element-polyfill": "^1.0.0",
    "tracked-toolbox": "^1.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-logical-assignment-operators": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^1.7.2",
    "@ember/test-waiters": "^2.4.3",
    "@embroider/test-setup": "^0.39.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "dotenv": "^8.2.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.25.3",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dotenv": "^3.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-qunit": "^5.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.6.1",
    "release-it-lerna-changelog": "^3.1.0",
    "tracked-maps-and-sets": "^2.2.1"
  },
  "resolutions": {
    "qunit": "~2.11.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=3.16.0"
    },
    "demoURL": "https://ember-google-maps.sandydoo.me",
    "paths": [
      "lib/in-repo-pin-addon"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking changes",
      "enhancement": ":star: Features",
      "bug": ":bug: Bug fixes",
      "documentation": ":book: Documentation",
      "internal": "Internal"
    }
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "14.16.1",
    "yarn": "1.22.10"
  }
}
