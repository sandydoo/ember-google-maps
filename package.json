{
  "name": "ember-google-maps",
  "version": "5.0.2",
  "description": "A friendly Ember addon for working with Google Maps.",
  "keywords": [
    "ember-addon",
    "emberjs",
    "google maps",
    "g-map",
    "google",
    "map",
    "maps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sandydoo/ember-google-maps.git"
  },
  "license": "MIT",
  "author": "Sander Melnikov <hey@sandydoo.me>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "homepage": "https://ember-google-maps.sandydoo.me",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/sandydoo"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:build": "qunit build-tests/build-test.js"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.0",
    "broccoli-debug": "^0.6.5",
    "broccoli-funnel": ">=3.0.8",
    "camelcase": "^6.0.0",
    "chalk": "^4.1.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-concurrency": "^2.1.0",
    "ember-modifier": "^3.0.0",
    "lodash": "^4.17.21",
    "tracked-maps-and-sets": "^3.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-logical-assignment-operators": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.5.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "dotenv": "^16.0.0",
    "ember-auto-import": "^2.4.1",
    "ember-cli": "~4.3.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dotenv": "^3.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.3.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "qunit": "^2.18.0",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.10.1",
    "release-it-lerna-changelog": "^4.0.1",
    "scenario-tester": "^2.0.1",
    "webpack": "^5.70.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=3.24.0"
    },
    "demoURL": "https://ember-google-maps.sandydoo.me",
    "paths": [
      "lib/in-repo-pin-addon"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking changes",
      "enhancement": ":star: Features",
      "bug": ":bug: Bug fixes",
      "documentation": ":book: Documentation",
      "internal": "Internal"
    }
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.14.2",
    "yarn": "1.22.18"
  }
}
