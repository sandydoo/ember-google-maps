{
  "name": "ember-google-maps",
  "version": "6.0.0",
  "description": "A friendly Ember addon for working with Google Maps.",
  "keywords": [
    "ember-addon",
    "emberjs",
    "google maps",
    "g-map",
    "google",
    "map",
    "maps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sandydoo/ember-google-maps.git"
  },
  "license": "MIT",
  "author": "Sander Melnikov <hey@sandydoo.me>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "homepage": "https://ember-google-maps.sandydoo.me",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/sandydoo"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:build": "qunit build-tests/build-test.js"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.0",
    "broccoli-debug": "^0.6.5",
    "broccoli-funnel": ">=3.0.8",
    "camelcase": "^6.3.0",
    "chalk": "^4.1.2",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-version-checker": "^5.1.2",
    "ember-concurrency": "^3.0.0",
    "ember-modifier": "^3.2.7",
    "lodash": "^4.17.21",
    "tracked-maps-and-sets": "^3.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.0",
    "ember-cli": "~4.12.1",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-dotenv": "^3.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.7.2",
    "ember-try": "^3.0.0-beta.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.5.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.7",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.10.3",
    "release-it-lerna-changelog": "^5.0.0",
    "scenario-tester": "^2.0.1",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "webpack": "^5.78.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=3.24.0"
    },
    "demoURL": "https://ember-google-maps.sandydoo.me"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking changes",
      "enhancement": ":star: Features",
      "bug": ":bug: Bug fixes",
      "documentation": ":book: Documentation",
      "dependencies": ":snake: Dependencies",
      "internal": "Internal"
    }
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.14.2",
    "yarn": "1.22.19"
  }
}
